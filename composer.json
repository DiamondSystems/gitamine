{
  "name": "gitamine/gitamine",
  "description": "git hooks manager",
  "type": "library",
  "license": "proprietary",
  "require": {
    "php": "^7.1.3",
    "ext-iconv": "*",
    "phpmd/phpmd": "^2.6",
    "phpunit/phpunit": "^6.5",
    "prooph/service-bus": "^6.1",
    "prooph/service-bus-symfony-bundle": "^0.5",
    "react/promise": "^2.5",
    "squizlabs/php_codesniffer": "^3.2",
    "symfony/console": "^4.0",
    "symfony/finder": "^4.0",
    "symfony/flex": "^1.0",
    "symfony/framework-bundle": "^4.0",
    "symfony/lts": "^4@dev",
    "symfony/phpunit-bridge": "^4.0",
    "symfony/stopwatch": "^4.0",
    "symfony/yaml": "^4.0"
  },
  "require-dev": {
    "j13k/yaml-lint": "^1.1",
    "mockery/mockery": "^1.0",
    "overtrue/phplint": "^1.0",
    "symfony/dotenv": "^4.0",
    "symfony/var-dumper": "^4.0"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "Gitamine\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-apcu": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "bin": [
    "bin/gitamine"
  ],
  "extra": {
    "symfony": {
      "id": "01C18WT0CW2SYQ8BPHZ8K25XXP",
      "allow-contrib": false
    }
  }
}
